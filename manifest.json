{
  "manifest_version": 2,
  "name": "Temporary Learner",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["app/js/content.js", "app/js/popup.js"]
    }
  ],
  "browser_action": {
    "default_icon": "app/images/demoIcon.png",
    "default_popup": "app/html/popup.html",
    "default_title": "A popup will come here"
  },
  "permissions": ["storage"]
}
